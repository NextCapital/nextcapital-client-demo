{
  "name": "external-api-demo",
  "version": "1.0.0",
  "description": "Demo of external NextCapital API and embeddable UI",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run copy && webpack",
    "copy": "cp -a ./static/ ./dist",
    "start": "npm run clean && npm run copy && webpack-dev-server --config=./webpack.config.js --watch",
    "start:sit": "NC_ENV=sit npm run start",
    "start:uat": "NC_ENV=uat npm run start"
  },
  "dependencies": {
    "highcharts": "^6.0.7",
    "lodash": "^4.17.15",
    "moment": "2.18.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
