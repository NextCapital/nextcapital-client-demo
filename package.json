{
  "name": "nextcapital-client-demo",
  "version": "1.0.0",
  "description": "Demo of the NextCapital Client",
  "author": "NextCapital Group",
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run copy && npm run build:styles && webpack",
    "build:styles": "node-sass ./styles/demo.scss ./dist/demo.css",
    "clean": "rm -rf dist",
    "copy": "cp -a ./static/ ./dist",
    "start": "npm run clean && npm run copy && npm run build:styles && webpack-dev-server --config=./webpack.config.js --watch"
  },
  "dependencies": {
    "@nextcapital/client": "^0.1.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.19",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "node-sass": "^4.14.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.9.0"
  }
}
